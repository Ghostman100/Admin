<h1>Listing users</h1>

<table>
  <tr>
    <th>User ID</th>
    <th>First name</th>
    <th>Last name</th>
    <th>Username</th>
    <th>In game?</th>
    <th>Updated at</th>
    <th></th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= user.user_id %></td>
      <td><%= user.first_name %></td>
      <td><%= user.last_name %></td>
      <td><%= user.username %></td>
      <td><%= user.in_game %></td>
      <td><%= user.updated_at %></td>
      <td><%= link_to 'Show', user_path(user) %></td>
    </tr>
  <% end %>
</table>